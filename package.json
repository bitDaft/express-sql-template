{
  "name": "nodejs-starter",
  "version": "0.7.0",
  "private": true,
  "type": "module",
  "description": "a nodejs express starter app boilerplate",
  "keywords": [
    "app",
    "objection",
    "knex",
    "express",
    "boilerplate"
  ],
  "engines": {
    "node": ">=16.11.1"
  },
  "homepage": "https://github.com/bitDaft",
  "license": "MIT",
  "author": "Tausif ali <tausif@bitdaft.dev> (https://github.com/bitDaft)",
  "funding": "https://ko-fi.com/bitdaft",
  "repository": {
    "type": "git",
    "url": "https://github.com/bitDaft/nodejs-starter.git"
  },
  "main": "./bin/www.js",
  "scripts": {
    "start": "NODE_ENV=production node ./bin/www.js",
    "dev": "nodemon ./bin/www.js",
    "migrate": "knex --esm migrate:latest -- ",
    "migrate:one": "knex --esm migrate:up -- ",
    "migrate:make": "knex --esm migrate:make -- ",
    "migrate:list": "knex --esm migrate:list -- ",
    "rollback": "knex --esm migrate:rollback -- ",
    "rollback:one": "knex --esm migrate:down -- ",
    "seed": "knex --esm seed:run -- ",
    "seed:make": "knex --esm seed:make -- ",
    "h": "knex --esm migrate:latest -- -h",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "imports": {
    "#": ".",
    "#app": "./app.js",
    "#config": "./config/index.js",
    "#middlewares": "./middlewares/index.js",
    "#models": "./database/models/index.js",
    "#conns": "./database/conn/index.js",
    "#routes": "./modules/_route.js",
    "#route/*": "./modules/*/*._route.js",
    "#controller/*": "./modules/*/*._controller.js",
    "#utils/*": "./utils/*.js",
    "#lib/*": "./lib/*.js",
    "#json": "./json/index.js",
    "#cache": "./utils/cache.js",
    "#knexfile": "./knexfile.js"
  },
  "devDependencies": {
    "dotenv": "^14.3.2",
    "nodemon": "^2.0.15",
    "pino-pretty": "^7.5.4"
  },
  "dependencies": {
    "axios": "^0.25.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-jwt": "^6.1.1",
    "form-data": "^4.0.0",
    "formidable": "^2.0.1",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^1.0.4",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.7.2",
    "objection": "^3.0.1",
    "objection-soft-delete": "^1.0.7",
    "pino": "^7.9.1",
    "rate-limiter-flexible": "^2.3.6",
    "uuid": "^8.3.2",
    "yargs": "^17.3.1"
  }
}
