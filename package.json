{
  "name": "nodejs-starter",
  "version": "0.9.4",
  "private": true,
  "type": "module",
  "description": "an opiniated nodejs express starter app boilerplate",
  "keywords": [
    "app",
    "objection",
    "knex",
    "express",
    "boilerplate"
  ],
  "homepage": "https://github.com/bitDaft",
  "license": "MIT",
  "author": "Tausif ali <tausif@bitdaft.dev> (https://github.com/bitDaft)",
  "funding": "https://ko-fi.com/bitdaft",
  "repository": {
    "type": "git",
    "url": "https://github.com/bitDaft/nodejs-starter.git"
  },
  "main": "./bin/www.js",
  "scripts": {
    "start": "run-p prod:**",
    "prod:app": "NODE_ENV=production node ./bin/www.js",
    "prod:worker:mail": "NODE_ENV=production node ./jobs/worker/mail.js",
    "prod:worker:general": "NODE_ENV=production node ./jobs/worker/general.js",
    "dev": "NODE_ENV=development nodemon ./bin/www.js",
    "dev-a": "run-p dev worker",
    "dev:www": "NODE_ENV=development nodemon ./bin/www.js",
    "startWorker": "nodemon ./jobs/worker/worker.js -- -w",
    "worker": "run-p worker:*",
    "worker:mail": "npm run startWorker mail",
    "worker:general": "npm run startWorker general",
    "worker:results": "npm run startWorker results",
    "migrate": "knex migrate:latest -- ",
    "migrate:one": "knex migrate:up -- ",
    "migrate:make": "knex migrate:make -- ",
    "migrate:list": "knex migrate:list -- ",
    "rollback": "knex migrate:rollback -- ",
    "rollback:one": "knex migrate:down -- ",
    "seed": "knex seed:run -- ",
    "seed:make": "knex seed:make -- ",
    "h": "node ./bin/www.js -- -h",
    "test": "echo \"Error: no test specified\" && exit 1",
    "FIX_ERR_REQUIRE_ESM_BULLMQ": "node -e \"['cjs','esm'].map(a=>'./node_modules/bullmq/dist/'+a+'/classes/child-processor.js').map(a=>require('fs').writeFileSync(a,require('fs').readFileSync(a,{encoding:'utf8'}).replace('require(processorFile)','await import(processorFile)')))\""
  },
  "imports": {
    "#": ".",
    "#app": "./app.js",
    "#config": "./config/index.js",
    "#middlewares": "./middlewares/index.js",
    "#models": "./database/models/index.js",
    "#conns": "./database/conn/index.js",
    "#file": "./loadFile/index.js",
    "#cache": "./utils/cache.js",
    "#knexfile": "./knexfile.js",
    "#routes": "./modules/_route.js",
    "#route/*": "./modules/*/*._route.js",
    "#controller/*": "./modules/*/*._controller.js",
    "#queue/*": "./jobs/queue/*.js",
    "#worker/*": "./jobs/worker/*.js",
    "#utils/*": "./utils/*.js",
    "#lib/*": "./lib/*.js"
  },
  "devDependencies": {
    "dotenv": "^16.0.3",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "pino-pretty": "^9.1.1",
    "supertest": "^6.3.1",
    "testcontainers": "^9.0.0"
  },
  "dependencies": {
    "axios": "^1.2.0",
    "bullmq": "^3.2.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-jwt": "^7.7.7",
    "formidable": "^2.0.1",
    "helmet": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.3.0",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.8.0",
    "npm-run-all": "^4.1.5",
    "objection": "^3.0.1",
    "pino": "^8.7.0",
    "rate-limiter-flexible": "^2.4.1",
    "uuid": "^9.0.0",
    "yargs": "^17.6.2"
  }
}
