{
  "name": "nodejs-starter",
  "version": "0.8.0",
  "private": true,
  "type": "module",
  "description": "a nodejs express starter app boilerplate",
  "keywords": [
    "app",
    "objection",
    "knex",
    "express",
    "boilerplate"
  ],
  "homepage": "https://github.com/bitDaft",
  "license": "MIT",
  "author": "Tausif ali <tausif@bitdaft.dev> (https://github.com/bitDaft)",
  "funding": "https://ko-fi.com/bitdaft",
  "repository": {
    "type": "git",
    "url": "https://github.com/bitDaft/nodejs-starter.git"
  },
  "main": "./bin/www.js",
  "scripts": {
    "start": "run-p prod:**",
    "prod:app": "NODE_ENV=production node ./bin/www.js",
    "prod:worker:mail": "NODE_ENV=production node ./jobs/worker/mail.js",
    "prod:worker:general": "NODE_ENV=production node ./jobs/worker/general.js",
    "dev": "NODE_ENV=development nodemon ./bin/www.js",
    "dev-a": "run-p dev:* worker:*",
    "dev:www": "NODE_ENV=development nodemon ./bin/www.js",
    "worker": "run-p worker:*",
    "worker:mail": "nodemon ./jobs/worker/mail.js",
    "worker:general": "nodemon ./jobs/worker/general.js",
    "migrate": "knex --esm migrate:latest -- ",
    "migrate:one": "knex --esm migrate:up -- ",
    "migrate:make": "knex --esm migrate:make -- ",
    "migrate:list": "knex --esm migrate:list -- ",
    "rollback": "knex --esm migrate:rollback -- ",
    "rollback:one": "knex --esm migrate:down -- ",
    "seed": "knex --esm seed:run -- ",
    "seed:make": "knex --esm seed:make -- ",
    "h": "node ./bin/www.js -- -h",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "imports": {
    "#": ".",
    "#app": "./app.js",
    "#config": "./config/index.js",
    "#middlewares": "./middlewares/index.js",
    "#models": "./database/models/index.js",
    "#conns": "./database/conn/index.js",
    "#file": "./loadFile/index.js",
    "#cache": "./utils/cache.js",
    "#knexfile": "./knexfile.js",
    "#routes": "./modules/_route.js",
    "#route/*": "./modules/*/*._route.js",
    "#controller/*": "./modules/*/*._controller.js",
    "#queue/*": "./jobs/queue/*.js",
    "#worker/*": "./jobs/worker/*.js",
    "#utils/*": "./utils/*.js",
    "#lib/*": "./lib/*.js"
  },
  "devDependencies": {
    "dotenv": "^14.3.2",
    "nodemon": "^2.0.15",
    "pino-pretty": "^7.6.0"
  },
  "dependencies": {
    "axios": "^0.25.0",
    "bullmq": "^1.77.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-jwt": "^6.1.1",
    "formidable": "^2.0.1",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^1.0.4",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.7.2",
    "npm-run-all": "^4.1.5",
    "objection": "^3.0.1",
    "pino": "^7.9.1",
    "rate-limiter-flexible": "^2.3.6",
    "uuid": "^8.3.2",
    "yargs": "^17.3.1"
  }
}
