{
  "exclude": ["node_modules", "node_modules/"],
  "compilerOptions": {
    "baseUrl": ".",
    "module": "es6",
    "target": "es6",
    "paths": {
      "#": ["."],
      "#app": ["./app.js"],
      "#config": ["./config/index.js"],
      "#common/*": ["./src/controllers/common/*.js"],
      "#middlewares": ["./src/middlewares/index.js"],
      "#models": ["./database/models/index.js"],
      "#conns": ["./database/conn/index.js"],
      "#routes": ["./src/routes/index.js"],
      "#routes/*": ["./src/routes/*/*.route.js"],
      "#controllers/*": ["./src/controllers/*/*.controller.js"],
      "#utils/*": ["./utils/*.js"],
      "#lib/*": ["./lib/*.js"],
      "#json": ["./json/index.js"],
      "#cache": ["./utils/cache.js"],
      "#knexfile": ["./knexfile.js"]
    },
    "resolveJsonModule": true,
    "moduleResolution": "node"
  }
}
